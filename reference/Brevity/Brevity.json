  "Air to Air weapon release": [
    {
      "type": "item",
      "term": "Fox One",
      "definition": "Indicates launch of a semi-active radar-guided missile (such as the AIM-7 Sparrow)."
    },
    {
      "type": "item",
      "term": "Fox Two",
      "definition": "Indicates launch of an infrared-guided missile (such as the AIM-9 Sidewinder)."
    },
    {
      "type": "item",
      "term": "Fox Three",
