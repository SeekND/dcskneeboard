  "ENGINE START-UP": [
    {
      "type": "item",
      "requirement": "Press “IDLE RELEASE” switch DOWN.",
      "action": "Safety gate is off and you are not prevented from shutting down your engine.",
      "location": "engine_start-up.png",
      "locationText": ""
    },
    {
      "type": "item",
      "requirement": "Throttle all the way",
      "action": "FULLY OPEN position",
      "location": "engine_start-up.png",
      "locationText": ""
    },
    {
      "type": "item",
      "requirement": "Throttle back",
      "action": "“START” position.",
      "location": "engine_start-up.png",
      "locationText": ""
    },
    {
      "type": "note",
      "text": "You will know you have reached this position when the “IDLE RELEASE” switch pops back up."
    },
    {
      "type": "item",
      "requirement": "“IDLE RELEASE” switch",
      "action": "released (UP). Throttle safety gate now prevents you from shutting down your engine.",
      "location": "engine_start-up.png",
      "locationText": ""
    },
    {
      "type": "note",
      "text": "Monitor DC Voltmeter for sufficient battery voltage. Battery starts can be made when voltages less than 24 volts are indicated, provided the voltage is not below 14 volts when cranking through 10% N1 speed."
    },
    {
      "type": "item",
      "requirement": "Lower collective",
      "action": "FULLY DOWN",
      "location": "engine_start-up.png",
      "locationText": ""
    },
    {
      "type": "item",
      "requirement": "Press and hold",
      "action": "starter switch",
      "location": "engine_start-up.png",
      "locationText": ""
    },
    {
      "type": "note",
      "text": "Wait until N1 (Gas Turbine Speed) RPM reaches 15 %."
    },
    {
      "type": "note",
      "text": "Check visually that the main rotor is turning as N1 reaches 15 percent."
    },
    {
      "type": "note",
      "text": "If the rotor is not turning, abort the start."
    },
    {
      "type": "item",
      "requirement": "At 15 % N1 RPM",
      "action": "throttle up to IDLE-STOP (START) position.",
      "location": "engine_start-up.png",
      "locationText": ""
    },
    {
      "type": "note",
      "text": "Engine light off occurs. Monitor EGT (Exhaust Gas Temperature) for any sign of a hot start."
    },
    {
      "type": "note",
      "text": "Wait until N1 RPM reaches 40 %."
    },
    {
      "type": "item",
      "requirement": "At 40 % N1 RPM",
      "action": "release start switch (do not hold it for more than 1 minute).",
      "location": "engine_start-up.png",
      "locationText": ""
    },
    {
      "type": "item",
      "requirement": "Wait until N1 RPM reaches 68-72 %. At 70 % N1 RPM",
      "action": "slowly throttle to FULLY OPEN",
      "location": "engine_start-up.png",
      "locationText": ""
    },
    {
      "type": "item",
      "requirement": "Set Inverter Switch",
      "action": "MAIN ON",
      "location": "engine_start-up.png",
      "locationText": ""
    },
    {
      "type": "item",
      "requirement": "Set Starter Generator Switch",
      "action": "STBY GEN",
      "location": "engine_start-up.png",
      "locationText": ""
    },
    {
      "type": "item",
      "requirement": "Wait until engine parameters stabilize",
      "action": "verify correct engine operation",
      "location": "engine_start-up.png",
      "locationText": ""
    },
    {
      "type": "note",
      "text": "EXH/EGT (Exhaust Gas Temperature): 400 -610 deg C"
    },
    {
      "type": "note",
      "text": "Engine Oil Temperature: below 100 deg C"
    },
    {
      "type": "note",
      "text": "Engine Oil Pressure: 80-100 psi"
    },
    {
      "type": "note",
      "text": "Transmission Oil Temperature: below 110 deg C"
    },
    {
      "type": "note",
      "text": "Transmission Oil Pressure: 40 -60 psi"
    },
    {
      "type": "note",
      "text": "Engine RPM: 6600 RPM"
    },
    {
      "type": "item",
      "requirement": "LOW RPM indicator light",
      "action": "Extinguished",
      "location": "engine_start-up.png",
      "locationText": ""
    },
    {
      "type": "item",
      "requirement": "Disconnect GPU (Ground Power Unit)",
      "action": "if used",
      "location": "engine_start-up.png",
      "locationText": ""
    },
    {
      "type": "item",
      "requirement": "Check for correct AC voltage",
      "action": "(112-118 V)",
      "location": "engine_start-up.png",
      "locationText": ""
    },
    {
      "type": "item",
      "requirement": "Check for correct DC voltage",
      "action": "(27-28.5 V",
      "location": "engine_start-up.png",
      "locationText": ""
    }
  ],
